:root {
  --icon-font-family: "{{fontName}}";
  {{#each codepoints}}
  --icon-{{@key}}: "\\{{this}}";
  {{/each}}
}

@font-face {
  font-family: "{{fontName}}";
  src: {{{src}}};
}

.{{baseClass}}::before {
  font-family: var(--icon-font-family) !important;
  font-style: normal;
  font-weight: normal;
  line-height: inherit;
  vertical-align: top;
}
{{#each codepoints}}

.{{../classPrefix}}{{@key}}::before {
  content: var(--{{../classPrefix}}{{@key}});
}
{{/each}}
