{{{open}}}#if inline-sprite{{{close}}}
<svg width="0" height="0" style="position:absolute">
	<defs>
    {{#shapes}}
      {{{svg}}}
    {{/shapes}}
  </defs>
</svg>
{{{open}}}#endif{{{close}}}

{{#shapes}}
{{{open}}}#if icon == {{name}} {{{close}}}
<svg viewBox="0 0 {{width.outer}} {{height.outer}}" class="Icon Icon--{{name}}">
	<use xlink:href="#{{name}}"></use>
</svg>
{{{open}}}/if{{{close}}}
{{/shapes}}
