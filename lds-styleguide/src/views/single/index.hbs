<div class="{{prefix}}Page-innerWrapper">
  <h1>Single: {{category}} / {{component}}</h1>
  {{#with (getProperty (getProperty this category) component)}}
    {{#equals this.category "base"}}
      {{> component:component}}
    {{/equals}}
    {{#equals this.category "component"}}
      {{> component:component}}
    {{/equals}}
    {{#equals this.category "module"}}
      {{> component:component}}
    {{/equals}}
    {{#equals this.category "view"}}
      {{> component:view}}
    {{/equals}}
    {{#equals this.category "helper"}}
      {{> component:component}}
    {{/equals}}
  {{/with}}
</div>
