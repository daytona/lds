<div class="Preview{{#if device}} Preview--{{device}}{{/if}} {{class}}" data-controller="Preview"{{#if device}}  data-device="{{device}}"{{/if}}>
  <div class="Preview-container js-container">

    <div class="Preview-device js-device">
      <div class="Preview-screen js-screen">
        <div class="Preview-browser js-browser" data-url="{{url}}">
          <iframe src="{{url}}" class="Preview-iframe js-iframe" id="{{iframeid}}"></iframe>
        </div>
      </div>
    </div>

  </div>
</div>
