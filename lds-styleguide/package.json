{
  "name": "lds-styleguide",
  "version": "1.0.0",
  "description": "Creating a styleguide based on LDS-structure to present all components, views and helpers in GUI ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run watch",
    "watch": "npm run watch:scripts & npm run watch:styles",
    "watch:styles": "nodemon -w src -q -e 'css' --exec 'lds build styles'",
    "watch:scripts": "nodemon -w src -q -e 'js' --exec 'lds build script'"
  },
