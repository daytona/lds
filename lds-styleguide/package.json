    "any-promise": "^1.1.0",
    "babel": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "eslint": "^1.10.3",
    "form-parse": "0.0.1",
    "fs": "0.0.2",
    "fs-readdir-recursive": "^1.0.0",
    "is-plain-object": "^2.0.1",
    "koa": "^1.2.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "marked": "^0.3.5",
    "mz": "^2.3.1",
    "webshot": "^0.18.0",
    "xhr-promise": "^2.0.0"
  }
